<Page x:Class="Jarvis.ProjectJarvis.Views.Pages.SidePanel.Profile.ProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Jarvis" Width="965" Height="700">

    <Page.Content>
        <Grid x:Name="ProfileGrid" >
            <StackPanel Margin="15,20,15,20">
                <Label Content="Добро пожаловать" HorizontalAlignment="Center"/>
                    <Label Content="Ваш логин:" FontSize="16" HorizontalAlignment="Center"/>
                    <TextBox Width="200" FontSize="16" HorizontalAlignment="Center"/>
                    <Label Content="Ваш пароль:" FontSize="16" HorizontalAlignment="Center"/>
                    <TextBox Width="200" FontSize="16" HorizontalAlignment="Center"/>
            </StackPanel>
        </Grid>
    </Page.Content>
</Page>